public class Palindrome {
    public static void main(String args[])
	{
		
		int a = 130;
		int org = a;
        int rem,rev=0;
        while (a!=0) {
            rem = a%10;
            rev=rev*10+rem;
            a = a/10;
        }
        if(rev==org){
            System.out.println("It's an Palindrome no");
        }
        else
            System.out.println("Its not an Palindrome no");
    }
}
