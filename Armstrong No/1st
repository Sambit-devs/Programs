public class Armstrong {
    public static void main(String args[])
	{
		
		int a = 153;
		int org = a;
        int next=0;
        int d;
        while (a!=0) {
            d = a%10;
            next=next+(d*d*d);
            a = a/10;
        }
        if(next==org){
            System.out.println("It's an armstrong no");
        }
        else
            System.out.println("Its not an armstrong no");
    }
}
